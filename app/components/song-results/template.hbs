<div id="song-list">
<ul>
{{#each songResults as |song|}}
  <li>{{song.title}}<button class="btn btn-default"{{action "playTrack" song.id}}>Play</button>
                         <button class="btn btn-default"{{action "stopTrack" song.id}}>Stop</button>
                         <button type="submit" {{action "submit" song}}class="btn btn-default">Use this song!</button></li>

{{/each}}
</ul>
</div>

{{yield}}
